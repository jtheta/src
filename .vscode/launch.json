{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "Current TS File",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/ts-node/dist/_bin.js",
        "args": ["-r", "tsconfig-paths/register", "${relativeFile}"],
        "cwd": "${workspaceRoot}",
        "protocol": "inspector"
    }
  ]
}